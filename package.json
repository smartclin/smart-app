		"analyze": "BUNDLE_ANALYZER_ENABLED=true next build",
		"auth:gen": "bunx @better-auth/cli generate --config src/lib/auth/index.ts --output prisma/schema/auth.prisma",
		"build": "next build",
		"seed": "tsx prisma/seed.ts",
		"check": "bunx biome check --write ",
		"check:unsafe": " biome check --write --unsafe src",
		"db:admin": "bunx tsx src/lib/db/admin.ts",
		"db:generate": "prisma generate --schema ./prisma/schema",
		"db:migrate": "prisma migrate dev",
		"db:push": "prisma db push --schema ./prisma/schema",
		"db:reset": "prisma migrate reset --force --schema ./prisma/schema --skip-seed",
		"db:studio": "prisma studio",
